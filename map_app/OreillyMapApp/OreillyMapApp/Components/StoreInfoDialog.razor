<MudDialog>
    <DialogContent>
        @if (Store != null)
        {
            <StoreCard Store="@Store" IconMode="@StoreCard.IconPosition.Bottom"></StoreCard>
        }
    </DialogContent>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    [Parameter] public Store Store { get; set; }
}